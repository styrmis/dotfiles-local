function this-branch-commits-by-file() {
  git diff --name-only master..head -- $1 |
  while read file_path; do
    echo $file_path

    git rev-list --reverse master..head -- $file_path |
    while read sha; do
      echo $(git rev-list --count master..$sha) ': ' $(git show --oneline --no-patch $sha)
    done

    echo
  done
}
